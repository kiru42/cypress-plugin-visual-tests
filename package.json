{
  "name": "@kiru42/cypress-plugin-visual-tests",
  "version": "1.5.37",
  "private": false,
  "description": "Cypress snapshot functionality for data",
  "homepage": "https://github.com/kiru42/cypress-plugin-visual-tests",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiru42/cypress-plugin-visual-tests.git"
  },
  "types": "types/index.d.ts",
  "files": [
    "*.js",
    "*.md",
    "assets",
    "docs",
    "LICENSE",
    "src",
    "types"
  ],
  "engines": {
    "node": ">=12.22.0"
  },
  "bugs": {
    "url": "https://github.com/kiru42/cypress-plugin-visual-tests/issues"
  },
  "keywords": [
    "cypress",
    "cypress-io",
    "cypress-plugin"
  ],
  "author": "Kiruban PREMKUMAR",
  "license": "MIT",
  "scripts": {
    "ci:test": "jest & npm --prefix cypress run ci:test",
    "cy:open": "npm --prefix cypress run cy:open",
    "cy:run": "npm --prefix cypress run cy:run",
    "cy:verify": "npm --prefix cypress run cy:verify",
    "jest": "jest",
    "lint": "eslint *.js",
    "start": "node cypress/test-server/index.js",
    "test": "jest & npm --prefix cypress run cy:run"
  },
  "dependencies": {
    "diff2html": "3.4.35",
    "fs-extra": "10.1.0",
    "image-size": "1.0.2",
    "jimp": "0.22.10",
    "js-base64": "3.7.2",
    "lodash": "4.17.21",
    "pixelmatch": "5.3.0",
    "pngjs": "6.0.0",
    "rand-token": "1.0.1",
    "rimraf": "3.0.2",
    "sanitize-filename": "1.6.3",
    "socket.io": "4.6.1",
    "socket.io-client": "4.6.1",
    "source-map-support": "0.5.21",
    "unidiff": "1.0.2"
  },
  "peerDependencies": {
    "cypress": "9.x"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.0",
    "eslint": "8.40.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.6.0",
    "jest": "29.1.1",
    "prettier": "2.8.8",
    "rewire": "6.0.0",
    "semantic-release": "24.1.3"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "plugins": [
      "cypress",
      "jest"
    ],
    "env": {
      "cypress/globals": true,
      "jest/globals": true
    },
    "rules": {
      "no-param-reassign": 0,
      "no-use-before-define": 0,
      "global-require": 0,
      "no-underscore-dangle": 0,
      "object-curly-newline": [
        "error",
        {
          "ObjectExpression": {
            "consistent": true
          },
          "ObjectPattern": {
            "consistent": true
          },
          "ImportDeclaration": {
            "multiline": true,
            "minProperties": 3
          },
          "ExportDeclaration": {
            "multiline": true,
            "minProperties": 3
          }
        }
      ]
    }
  },
  "eslintIgnore": [
    "cypress/**/*"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/examples/",
      "<rootDir>/node_modules/",
      "<rootDir>/cypress/"
    ],
    "clearMocks": true,
    "testEnvironment": "node"
  }
}
